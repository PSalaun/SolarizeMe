<div class="home_banner_wrapper">
  <div class="container-fluid">
    <div class="home_banner">
      <div class="container home_banner_content">
        <h1>SolarizeMe</h1>
        <h2>Finding a home to your solar panel!</h2>
        <%= link_to "Start investing now", projects_path, class: "btn_projects" %>
        <%= link_to "Learn more", learn_more_path, class: "btn_learn-more" %>
      </div>
    </div>
  </div>
</div>

<% @projects.each do |project| %>
  <h3><%= project.name %> - <%= project.end_date %></h3>
<% end %>
